// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./main-db.sqlite"
}

model Product {
  id      Int @id @default(autoincrement())
  name    String
  price   Float
  active  Boolean @default(false)
}